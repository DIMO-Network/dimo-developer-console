{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "env": {
    "jest": true
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "next": {
      "rootDir": "./src"
    }
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": "error",
    "no-console": "off"
  }
}
